		{
			"name": "Launch Package",
			"type": "node",
			"request": "launch",
			"program": "${fileDirname}/index.node.js",
			"stopOnEntry": false,
			"args": [],
			"cwd": "${workspaceRoot}",
			"preLaunchTask": null,
			"runtimeExecutable": null,
			"runtimeArgs": [
				"--nolazy"
			],
			"sourceMaps": true
		},
		{
			"name": "Attach",
			"type": "node",
			"request": "attach",
			"port": 5858,
			"address": "localhost",
			"restart": false,
			"localRoot": "${workspaceRoot}",
			"remoteRoot": null
