{
  "name": "ubc-farm-utils",
  "version": "1.0.0",
  "description": "Utilities used by various ubc-farm sibling packages",
  "license": "MIT",
  "repository": "https://github.com/NotWoods/ubc-farm/tree/master/packages/ubc-farm-utils",
	"main": "index.node.js",
	"jsnext:main": "index.js",
	"private": true,
	"devDependencies": { "rollup": "^0.34.1" },
  "scripts": {
		"build": "rollup -i index.js -o index.node.js -f cjs -m",
		"build:es": "rollup -i index.js -o index.es.js -f es -m",
		"prepublish": "npm run build && npm run build:es"
	}
}